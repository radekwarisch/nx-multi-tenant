          "options": {
            "lintFilePatterns": ["apps/world/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "world-container": {
      "root": "libs/world-container",
      "sourceRoot": "libs/world-container/src",
      "projectType": "library",
      "targets": {
        "lint": {
