          "options": {
            "lintFilePatterns": ["libs/app-core/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/app-core"],
          "options": {
            "jestConfig": "libs/app-core/jest.config.js",
            "passWithNoTests": true
