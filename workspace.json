            "root": "apps/world",
            "outputPath": "dist/apps/world"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "world:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "world:build:production",
