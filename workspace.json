    "social-qa-fr": {
      "root": "apps/social-qa/fr",
      "sourceRoot": "apps/social-qa/fr/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/social-qa/fr",
            "index": "apps/social-qa/fr/src/index.html",
            "main": "apps/social-qa/fr/src/main.tsx",
            "polyfills": "apps/social-qa/fr/src/polyfills.ts",
            "tsConfig": "apps/social-qa/fr/tsconfig.app.json",
            "assets": [
              "apps/social-qa/fr/src/favicon.ico",
              "apps/social-qa/fr/src/assets"
            ],
            "styles": ["apps/social-qa/fr/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/social-qa/fr/src/environments/environment.ts",
                  "with": "apps/social-qa/fr/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "social-qa-fr:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "social-qa-fr:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/social-qa/fr/**/*.{ts,tsx,js,jsx}"]
          }
