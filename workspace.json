{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "core-container": {
      "root": "libs/core-container",
      "sourceRoot": "libs/core-container/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/core-container/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core-container"],
          "options": {
            "jestConfig": "libs/core-container/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "eu-container": {
